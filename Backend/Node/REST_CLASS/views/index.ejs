


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>REST API</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <h1 class="heading">Quora Posts</h1>
    <% for(post of posts){ %>
        <div>
            <h3 class="username">@<%= post.username %></h3>
            <h4 class="content"> <%= post.content %></h4>
            <a href="http://localhost:8080/post/<%=post.id%>">view in detail</a>
        </div>
    <% }; %>
    <a href="http://localhost:8080/post/new"> Create new post</a>
</body>
</html>